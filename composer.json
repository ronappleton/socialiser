{
  "name": "ronappleton/socialiser",
  "description": "Extends Laravels Socialite with extra providers and usage",
  "keywords": ["oauth", "laravel", "statistics"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ron Appleton",
      "email": "ron.appleton@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/contracts": "~5.4",
    "illuminate/http": "~5.4",
    "illuminate/support": "~5.4",
    "league/oauth1-client": "~1.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "autoload": {
    "psr-4": {
      "RonAppleton\\Socialiser\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "RonAppleton\\Socialiser\\SocialiserServiceProvider"
      ],
      "aliases": {
        "Socialiser": "RonAppleton\\Socialiser\\Facades\\Socialiser"
      }
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists(base_path() . '/config/socialiser.php) || copy(base_path() . '/vendor/ronappleton/socialiser/src/config/socialiser.php, base_path() . '/config/socialiser.php');\""
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
